<h3>Band Info</h3>
<%= @band.name %>
<%= link_to "Edit Info", edit_band_url(@band) %>

<ul>
  <li>Albums
    <% @band.albums.each do |album| %>
      <p><%= link_to "#{album.title}", album_url(album) %></p>
    <% end %>
  </li>
</ul>
<br>

<%= button_to "Create New Album!", new_album_url, method: :get %>

<%= link_to "<< All Bands", bands_url %>
